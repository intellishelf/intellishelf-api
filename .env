# MongoDB Configuration
MongoDB__ConnectionString=mongodb+srv://[username]:[password]@[cluster-url]/[database]?retryWrites=true&w=majority&appName=[app-name]
MongoDB__DatabaseName=[database-name]

# JWT Configuration
Jwt__Key=[at-least-32-characters-long-secret-key]
Jwt__ExpirationDays=30

# Azure Vision Configuration
AzureVision__Endpoint=https://[your-instance].cognitiveservices.azure.com
AzureVision__Key=[your-azure-vision-key]

# OpenAI Configuration
OpenAI__ApiKey=[your-openai-key] 